{
  "name": "stride",
  "description": "Isomorphic react.",
  "version": "5.4.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/psastras/stride"
  },
  "homepage": "https://github.com/psastras/stride",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "starter",
    "boilerplate",
    "template",
    "webpack",
    "koa",
    "transmit"
  ],
  "main": "dist/server.js",
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./dist/server.js",
    "build-server": "webpack --colors --display-error-details --config configs/webpack.server.js",
    "build-client": "webpack --colors --display-error-details --config configs/webpack.client.js",
    "build": "concurrently \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "webpack --watch --verbose --colors --display-error-details --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/*.js\" && npm run start",
    "watch-client": "webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "concurrently --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\""
  },
  "dependencies": {
    "alt": "0.18.4",
    "alt-container": "1.0.2",
    "babel-polyfill": "6.6.1",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "isomorphic-fetch": "2.2.1",
    "isomorphic-style-loader": "1.0.0",
    "json-format": "0.1.2",
    "koa": "1.2.0",
    "koa-proxy": "0.5.0",
    "koa-static": "2.0.0",
    "material-colors": "1.0.0",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-highlight": "0.8.0",
    "react-inline-css": "2.1.0",
    "react-markdown": "2.3.0",
    "react-router": "2.0.0",
    "react-router-transition": "0.0.5",
    "react-transmit": "3.1.7",
    "style-loader": "0.13.1"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.6.5",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "concurrently": "2.0.0",
    "forever": "0.15.1",
    "json-loader": "0.5.4",
    "just-wait": "1.0.5",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1",
    "webpack-node-externals": "1.0.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
